y = \"block\"\n    //     setTimeout(hideToast,2000)\n    // }\n    // const hideToast = () => {\n    //     const toast = document.getElementById('toast');\n    //     toast.style.display = \"none\"\n    // }\n    return (\n        <div className=\"addContainer md:w-1/3 md:mx-auto mx-3 mt-3 flex justify-center\">\n            <div className='w-11/12'>\n                <form onSubmit={handleAdd}>\n                    <div>\n                        <label htmlFor=\"title\">Title</label>\n                        <input\n                            type=\"text\"\n                            name=\"title\"\n                            id=\"title\"\n                            value={title}\n                            required\n                            onChange={(e) => setTitle(e.target.value)}\n                            className='bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500' />\n                    </div>\n                    <div className='my-3'>\n                        <label htmlFor=\"description\">Description</label>\n                        <textarea\n                            rows={5}\n                            name=\"description\"\n                            id=\"description\"\n                            value={description}\n                            required\n                            onChange={(e) => setDescription(e.target.value)}\n                            style={{ resize: \"none\" }}\n                            className='bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500' />\n                    </div>\n                    <div className='flex justify-center'>\n                        <button\n                            type='submit'\n                            className=' bg-blue-700 rounded-md text-white px-5 py-1 '\n                        >Add</button>\n                    </div>\n                </form>\n                <div className=\"toast bg-green-600 text-white p-3 rounded-xl shadow-2xl text-center absolute bottom-4 left-1/2 -translate-x-1/2\" id='toast'>\n                    <p>This is test</p>\n                </div>\n            </div>\n        </div>\n    );\n}\n\nexport default CreateTask;","/Users/rajasurya/Desktop/ALL FIles/Projects/mern-todo-app/frontend/src/components/Task/Task.jsx",[],[],{"ruleId":"112","severity":1,"message":"113","line":7,"column":39,"nodeType":"114","messageId":"115","endLine":7,"endColumn":51},{"ruleId":"112","severity":1,"message":"113","line":8,"column":38,"nodeType":"114","messageId":"115","endLine":8,"endColumn":50},{"ruleId":"112","severity":1,"message":"116","line":16,"column":31,"nodeType":"114","messageId":"115","endLine":16,"endColumn":34},"no-unused-vars","'userDispatch' is assigned a value but never used.","Identifier","unusedVar","'res' is assigned a value but never used."]